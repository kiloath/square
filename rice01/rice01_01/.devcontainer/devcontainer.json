{
	"name": "Rust",
	"image": "korustbullseye",
	"features": {
		"ghcr.io/lee-orr/rusty-dev-containers/fermyon-spin:0": {},
		"ghcr.io/devcontainers/features/docker-in-docker:2": {}
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"vadimcn.vscode-lldb",
				"rust-lang.rust-analyzer"
			]
		}
	},
	"runArgs": ["--init"],
	"postCreateCommand": "docker pull ghcr.io/cross-rs/x86_64-pc-windows-gnu:0.2.5"
}
